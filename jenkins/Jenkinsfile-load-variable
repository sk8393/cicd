pipeline {
	agent { label "master" }

	environment {
		REPOSITORY = "cicd"
	}

	stages {
		stage("clean workspace") {
			steps {
				cleanWs()
			}
		}

		stage("git clone") {
			steps {
				sh "git clone https://github.com/sk8393/cicd.git"
				sh "pwd"
				sh "ls -lR"
			}
		}

		stage("build image") {
			steps {
				def gitbucket_url = sh(script: 'cd cicd/;cat variables.part.sh | grep GITBUCKET_URL | cut -d "=" -f2 | sed -e "s/\"//g"', returnStdout: true)
			}
		}
	}
}

